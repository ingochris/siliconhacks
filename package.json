{
  "name": "siliconhacks",
  "version": "1.0.0",
  "description": "SiliconHacks website. Written in HTML, CSS, Node.js, express, and React.",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "build": "browserify src/main.jsx -t [ babelify --presets [ es2015 react ] ] | uglifyjs -cm  --noerr --warnings=false > public/js/main.js",
    "watch": "watchify src/main.jsx -v -t [ babelify --presets [ es2015 react ] ] -o public/js/main.js",
    "test": "npm run build & npm run start"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "eslint": "^2.13.1",
    "eslint-plugin-html": "^1.5.2",
    "express": "^4.14.0",
    "history": "^2.1.2",
    "lodash": "^4.15.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.7.0",
    "uglify-js": "^2.7.3",
    "watchify": "^3.7.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-html": "^1.4.0",
    "eslint-plugin-react": "^4.3.0",
    "imagemin": "^5.2.2",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-pngquant": "^5.0.0",
    "uglify-js": "^2.6.2"
  }
}
